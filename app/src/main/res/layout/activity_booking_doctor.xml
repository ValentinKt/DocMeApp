<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="bookingDoctorVM" type="com.docmeapp.app.modules.bookingdoctor.data.viewmodel.BookingDoctorVM"/>
    </data>
    <LinearLayout android:id="@+id/linearBookingDoctor"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarAppbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAppointment"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{bookingDoctorVM.bookingDoctorModel.txtAppointment}"
                style="@style/txtRalewayromansemibold16" tools:text="@string/lbl_appointment"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnrectangle959"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_28pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh" android:paddingTop="@dimen/_28pxv">
            <LinearLayout android:id="@+id/linearProfile"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangle959"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_115pxh"
                    android:layout_width="@dimen/_115pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_8pxh}"
                    imageRes="@{@drawable/img_rectangle959_111x111}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle959_111x111"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumndrmarcushorizonone"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDrMarcusHorizonOne"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtDrMarcusHorizonOne}"
                        style="@style/txtRalewayromansemibold18_1" tools:text="@string/msg_dr_marcus_hori"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtChardiologist"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{bookingDoctorVM.bookingDoctorModel.txtChardiologist}"
                        style="@style/txtRalewayromanmedium14_2" tools:text="@string/lbl_chardiologist"/>
                    <LinearLayout android:id="@+id/linearRowstar"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageStar"
                            android:layout_gravity="top"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_star"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFortySeven"
                            android:layout_gravity="bottom"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{bookingDoctorVM.bookingDoctorModel.txtFortySeven}"
                            style="@style/txtRalewayromanmedium13_1" tools:text="@string/lbl_4_7"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowlocation"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageLocation"
                            android:layout_gravity="top"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_location"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtDistance"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{bookingDoctorVM.bookingDoctorModel.txtDistance}"
                            style="@style/txtRalewayromanmedium14_2" tools:text="@string/lbl_800m_away"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowdate"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDate"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtDate}"
                    style="@style/txtRalewayromansemibold16" tools:text="@string/lbl_date"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtChange"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtChange}"
                    style="@style/txtRalewayromanregular14_2" tools:text="@string/lbl_change"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowcalendar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCalendar"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_width="@dimen/_36pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_calendar"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom" android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtPrice}"
                    style="@style/txtRalewayromansemibold14_2" tools:text="@string/msg_wednesday_jun"/>
            </LinearLayout>
            <View android:background="@color/gray_100_01"
                android:id="@+id/lineLine"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_327pxh"/>
            <LinearLayout android:id="@+id/linearRowreason"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtReason"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtReason}"
                    style="@style/txtRalewayromansemibold16" tools:text="@string/lbl_reason"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtChangeOne"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtChangeOne}"
                    style="@style/txtRalewayromanregular14_2" tools:text="@string/lbl_change"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowshare"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_blue_50_01_radius_18"
                    android:id="@+id/btnShare"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_width="@dimen/_36pxh"
                    android:paddingHorizontal="@dimen/_8pxh"
                    android:paddingVertical="@dimen/_8pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_share" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:gravity="bottom"
                    android:id="@+id/txtChestpain"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtChestpain}"
                    style="@style/txtRalewayromansemibold14_3" tools:text="@string/lbl_chest_pain"/>
            </LinearLayout>
            <View android:background="@color/gray_100_01"
                android:id="@+id/lineLineOne"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_327pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPaymentDetail"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{bookingDoctorVM.bookingDoctorModel.txtPaymentDetail}"
                style="@style/txtRalewayromansemibold16" tools:text="@string/lbl_payment_detail"/>
            <LinearLayout android:id="@+id/linearConsultation"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtConsultation"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtConsultation}"
                    style="@style/txtRalewayromanregular14_3" tools:text="@string/lbl_consultation"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceOne"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtPriceOne}"
                    style="@style/txtRalewayromanregular14" tools:text="@string/lbl_60_00"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAdminfee"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAdminFee"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtAdminFee}"
                    style="@style/txtRalewayromanregular14_3" tools:text="@string/lbl_admin_fee"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceTwo"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtPriceTwo}"
                    style="@style/txtRalewayromanregular14" tools:text="@string/lbl_01_00"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearAdditonal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAditionalDiscount"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtAditionalDiscount}"
                    style="@style/txtRalewayromanregular14_3" tools:text="@string/msg_aditional_disco"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOne"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtOne}"
                    style="@style/txtRalewayromanregular14_4" tools:text="@string/lbl"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearTotal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTotal"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtTotal}"
                    style="@style/txtRalewayromansemibold14_3" tools:text="@string/lbl_total"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceThree"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtPriceThree}"
                    style="@style/txtRalewayromansemibold14_1" tools:text="@string/lbl_61_00"/>
            </LinearLayout>
            <View android:background="@color/gray_100_01"
                android:id="@+id/lineLineTwo"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_319pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPaymentMethod"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{bookingDoctorVM.bookingDoctorModel.txtPaymentMethod}"
                style="@style/txtRalewayromansemibold16" tools:text="@string/lbl_payment_method"/>
            <LinearLayout android:id="@+id/linearCard"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingHorizontal="@dimen/_14pxh"
                android:paddingVertical="@dimen/_14pxv" style="@style/groupStylewhite_8">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtVISA"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtVISA}"
                    style="@style/txtInterblackitalic16" tools:text="@string/lbl_visa"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtChangeTwo"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtChangeTwo}"
                    style="@style/txtRalewayromanregular12" tools:text="@string/lbl_change"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowtotalone"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTotal1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTotalOne"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtTotalOne}"
                    style="@style/txtRalewayromanmedium14" tools:text="@string/lbl_total"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceFour"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{bookingDoctorVM.bookingDoctorModel.txtPriceFour}"
                    style="@style/txtRalewayromansemibold18_1" tools:text="@string/lbl_61_002"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_blue_600_radius_tl_8_bl_8_br_8"
                android:ellipsize="end"
                android:fontFamily="@font/raleway"
                android:gravity="center" android:id="@+id/btnBooknow"
                android:layout_height="@dimen/_50pxv"
                android:layout_width="@dimen/_192pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_61pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_book_now"
                android:textAllCaps="false"
                android:textColor="@color/white_A700"
                android:textFontWeight="600"
                android:textSize="@dimen/_14pxh"
                android:textStyle="bold" tools:text="@string/lbl_book_now"/>
        </LinearLayout>
    </LinearLayout>
</layout>
